				<celda>308</celda>
				<celda>874</celda>
				<celda>989</celda>
			</fila>
		</tarjeta>
	</tarjetas>
	<!-- clientes contiene la lista de clientes (cero o mas cliente) -->
	<clientes>
		<!-- cliente representa la información de un cliente -->
		<!-- El atributo usuario contiene el nombre del usuario al que pertenece el cliente -->
		<cliente usuario="paco">
