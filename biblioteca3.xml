<?xml version="1.0" encoding="UTF-8"?>
<!-- Elemento raiz del documento -->
<biblioteca>
  <!-- usuarios representa la lista de usuarios. Contiene uno o mas usuario -->
	<usuarios>
		<!-- El elemento usuario representa los datos de un usuario -->
		<usuario id="paco" nombre="Pako Makako" />
	</usuarios>
	<!-- tarjetas es la lista de tarjetas contiene una o mas tarjeta -->
	<tarjetas>
		<!-- El elemento tarjeta representa la información de una tarjeta -->
		<!-- usuario contiene el nombre del usuario al que pertenece la tarjeta -->
		<tarjeta usuario="paco">
			<!-- fila contiene las celdas de una fila de la tarjeta de claves -->
			<fila>
				<!-- celda contiene una clave -->
